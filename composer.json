    "keywords": ["Laravel", "Quiz", "Questionnaire", "Question"],
    "require": {
        "php" : ">=7.0",
        "ghunti/highcharts-php": "*",
        "laracasts/flash": "*",
        "laravelcollective/html": "*",
        "predis/predis": "*",
        "yajra/laravel-datatables-buttons": "*",
        "yajra/laravel-datatables-html": "*",
        "yajra/laravel-datatables-oracle": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3.0",
        "sempro/phpunit-pretty-print": "^1.0",
        "symplify/easy-coding-standard": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "PandoApps\\Quiz\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PandoApps\\Quiz\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "PandoApps\\Quiz\\QuizServiceProvider",
                "Yajra\\DataTables\\DataTablesServiceProvider",
                "Yajra\\DataTables\\ButtonsServiceProvider",
                "Yajra\\DataTables\\HtmlServiceProvider"
            ],
            "aliases": {
                "Quiz": "PandoApps\\Quiz\\Facades\\Quiz"
            }
        }
    },
    "minimum-stability": "dev",
    "license": "MIT"
